{
  "name": "alcodo/alpaca",
  "description": "Simple laravel CMF system",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cms",
    "cmf",
    "laravel"
  ],
  "homepage": "https://github.com/alcodo/user",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.2",
    "artesaos/seotools": "^0.9",
    "laracasts/flash": "^3.0",
    "msurguy/honeypot": "^1.0",
    "spatie/laravel-permission": "^2.7.0",
    "cocur/slugify": "^3.1",
    "approached/laravel-date-international": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Alpaca\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alpaca\\AlpacaServiceProvider"
      ]
    }
  }
}
