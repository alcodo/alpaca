{
  "name": "alcodo/alpaca",
  "description": "Simple laravel CMF system",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "cmf",
    "cms",
    "laravel"
  ],
  "homepage": "https://github.com/alcodo/user",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": "~5.1",
    "artesaos/seotools": "^0.9",
    "zizaco/entrust": "5.2.x-dev",
    "cocur/slugify": "^2.1",
    "adamwathan/bootforms": "^0.8.0",
    "approached/laravel-date-international": "^1.1",
    "laracasts/flash": "^2.0",
    "msurguy/honeypot": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Alpaca\\": "./src/"
    },
    "classmap": [
      "src/resources/tests/AlpacaTestCase.php"
    ],
    "files": [
      "./src/helper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "dev"
}
